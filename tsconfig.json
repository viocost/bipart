{
  "include": ["src/**/*"],
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "declaration": true,
    "target": "es5",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules", "dist", "**/*.test.ts"]


}
